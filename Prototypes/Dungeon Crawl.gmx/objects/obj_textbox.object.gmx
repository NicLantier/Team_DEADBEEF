        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Input controller
if(type){    
    if(keyboard_check(vk_enter)){
        if((string_upper(txt) == "GO LEFT" || string_upper(txt) == "GOLEFT") &amp;&amp; !place_meeting(obj_player.x+obj_player.player_padding, obj_player.y, obj_wall)){
            draw_set_color(c_yellow);
            obj_player.hspeed -= player_speed;
            type = false;
            alarm[0] = room_speed * seconds;
        }
        else if(string_upper(txt) == "GO RIGHT" || string_upper(txt) == "GORIGHT"){
            if(position_meeting(obj_player.x, obj_player.y, obj_wall)){
                obj_player.hspeed = 0;
            } else {
                draw_set_color(c_yellow);
                obj_player.hspeed += player_speed;
                type = false;
                alarm[0] = room_speed * seconds;
            }
        }
        else if(string_upper(txt) == "GO UP" || string_upper(txt) == "GOUP"){
            draw_set_color(c_yellow);
            obj_player.vspeed -= player_speed;
            type = false;
            alarm[0] = room_speed * seconds;
        }
        else if(string_upper(txt) == "GO DOWN" || string_upper(txt) == "GODOWN"){
            draw_set_color(c_yellow);
            obj_player.vspeed += player_speed;
            type = false;
            alarm[0] = room_speed * seconds;
        }
        else {
            txt = "";
            keyboard_string = "";
        }
    }
    
    if(string_length(keyboard_string) &lt; max_length){
        txt = keyboard_string;
    } else {
        keyboard_string = txt;
    }
}
</string>
