with obj_player {
        if (hspeed != 0 and !place_free(x+hspeed, y)){
        if (hspeed &gt; 0){ move_contact_solid(0, hspeed+player_padding);}
        if (hspeed &lt; 0){ move_contact_solid(180, -hspeed-player_padding);}
        hspeed = 0;
        obj_textbox.txt = "";
        obj_textbox.keyboard_string = "";
    }

    if (vspeed != 0 and !place_free(x+hspeed, y+vspeed)){
        if (vspeed &gt; 0){ move_contact_solid(270, vspeed);}
        if (vspeed &lt; 0){ move_contact_solid(90, -vspeed);}
        vspeed = 0;
        obj_textbox.txt = "";
        obj_textbox.keyboard_string = "";
    }
}
